{
    "mockDirectory": "./mock",
    "quiet": false,
    "port": "7880",
    "latency": 50,
    "logRequestHeaders": false,
    "webServices": {
    "": {
      "mockFile": "sales.json",
      "latency": 20,
      "verbs": ["post"],
      "responses": {
        "post": {
          "mockFile": "sales.json"
        }
      }
    },
    "*/refund": {
      "mockFile": "refunds.json",
      "latency": 20,
      "verbs": ["post"],
      "responses": {
